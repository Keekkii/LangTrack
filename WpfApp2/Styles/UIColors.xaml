<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Pozadina -->
    <Color x:Key="primaryBackColor1">#001A17</Color>
    <Color x:Key="primaryBackColor2">#1B1448</Color>
    <Color x:Key="secondaryBackColor1">#00453F</Color>
    <Color x:Key="secondaryBackColor2">#001A17</Color>

    <!-- Rub prozora -->
    <Color x:Key="winBorderColor1">#18A27A</Color>
    <Color x:Key="winBorderColor2">#F4A261</Color>
    <Color x:Key="winBorderColor3">#56C596</Color>

    <!-- Paleta boja -->
    <SolidColorBrush x:Key="color1" Color="#A3F7B5"/>
    <SolidColorBrush x:Key="color2" Color="#34D399"/>
    <SolidColorBrush x:Key="color3" Color="#00B4D8"/>
    <SolidColorBrush x:Key="color4" Color="#F4A261"/>
    <SolidColorBrush x:Key="color5" Color="#89CFF0"/>
    <SolidColorBrush x:Key="color6" Color="#F8D210"/>
    <SolidColorBrush x:Key="color7" Color="#EE6C4D"/>
    <SolidColorBrush x:Key="color8" Color="#3FA7D6"/>
    <SolidColorBrush x:Key="color9" Color="#8AC926"/>
    <SolidColorBrush x:Key="color10" Color="#FF6B6B"/>

    <!-- Font boje -->
    <SolidColorBrush x:Key="titleColor1" Color="#D8F3DC"/>
    <SolidColorBrush x:Key="titleColor2" Color="#B7E4C7"/>
    <SolidColorBrush x:Key="titleColor3" Color="#95D5B2"/>
    <SolidColorBrush x:Key="plainTextColor1" Color="#74C69D"/>
    <SolidColorBrush x:Key="plainTextColor2" Color="#52B788"/>
    <SolidColorBrush x:Key="plainTextColor3" Color="#40916C"/>

    <!-- Panel -->
    <SolidColorBrush x:Key="panelColor" Color="#002924"/>
    <SolidColorBrush x:Key="panelOverColor" Color="#004037"/>
    <SolidColorBrush x:Key="panelActiveColor" Color="#005247"/>

</ResourceDictionary>
